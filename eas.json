{
  "cli": {
    "version": ">= 10.0.2"
  },
  "build": {
    "staging": {
      "distribution": "internal",
      "channel": "staging",
      "android": {
        "applicationArchivePath": "android/app/build/outputs/apk/release/app-release.apk",
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_APP_MODE": "staging"
      }
    },
    "production": {
      "channel": "production",
      "node": "20.15.0",
      "env": {
        "EXPO_PUBLIC_APP_MODE": "production"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
